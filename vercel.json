{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/rooms/(.*)",
      "dest": "/api"
    },
    {
      "src": "/holds",
      "dest": "/api"
    },
    {
      "src": "/reservations/(.*)",
      "dest": "/api"
    },
    {
      "src": "/reservations",
      "dest": "/api"
    },
    {
      "src": "/api",
      "dest": "/api"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/public/$1.css"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/$1.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}